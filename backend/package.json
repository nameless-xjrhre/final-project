{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx src",
    "migrate": "prisma migrate dev",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
    "studio": "prisma studio",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test:integration": "npm run migrate:test && dotenv -e .env.test jest -i",
    "test:unit": "jest -i -c jest.unit.config.js",
    "test-unit:watch": "npm run test:unit -- --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "get-port": "^5.1.1",
    "graphql-request": "^4.2.0",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "prisma": "^3.12.0",
    "sqlite3": "^5.0.5",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "graphql-scalars": "^1.17.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
